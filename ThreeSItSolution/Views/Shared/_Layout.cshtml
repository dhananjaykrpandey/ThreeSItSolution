@{
    ViewData["Title"] = "3S IT Training and Business Solutions";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>@ViewData["Title"]</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <meta content="" name="keywords">
    <meta content="" name="description">

    <!--<strong>HostingEnvironment.EnvironmentName is not Development</strong>-->
    <!-- Favicons -->
    <link href="~/img/LogoIcon48X48.jpg" rel="icon">
    <link href="~/img/LogoIcon48X48.jpg" rel="apple-touch-icon">


    <!-- Google Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="dns-prefetch" href="https://fonts.googleapis.com">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,700,700i|Montserrat:300,400,500,700" rel="stylesheet">

    <!-- Bootstrap CSS File -->
    <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Libraries CSS Files -->
    <link href="~/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/lib/animate/animate.min.css" rel="stylesheet">
    <link href="~/lib/ionicons/css/ionicons.min.css" rel="stylesheet">
    <link href="~/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/lib/lightbox/css/lightbox.min.css" rel="stylesheet">

    <environment names="Staging,Production">
        <!-- Main Stylesheet File -->
        <link href="~/css/style.min.css" rel="stylesheet">
    </environment>
    <environment names="Development">
        <!-- Main Stylesheet File -->
        <link href="~/css/style.css" rel="stylesheet">
    </environment>

    <script src="~/lib/jquery/jquery.min.js"></script>
    <!-- =======================================================
      Theme Name: NewBiz
      Theme URL: https://bootstrapmade.com/newbiz-bootstrap-business-template/
      Author: BootstrapMade.com
      License: https://bootstrapmade.com/license/
    ======================================================= -->
</head>

<body>

    <!--==========================
    Header
    ============================-->
    <header id="header" class="fixed-top">
        <div class="container">

            <div class="logo float-left">
                <!-- Uncomment below if you prefer to use an image logo -->

                <a asp-action="Index" asp-controller="Home" asp-fragment="intro" class="scrollto"> <h6 class="text-dark"><img src="~/img/LogoIconMain.png" alt="" class="img-fluid"><span> @ViewData["Title"]</span></h6></a>
                @*<h5 class="text-light"><a href="#header"><span>@ViewData["Title"]</span></a></h5>*@
            </div>

            <nav class="main-nav float-right d-none d-lg-block">
                <ul>
                    <li class="active"><a asp-action="Index" asp-controller="Home">Home</a></li>
                    <li><a asp-action="Index" asp-controller="Home" asp-fragment="about">About Us</a></li>


                    <li class="drop-down">
                        <a asp-action="Index" asp-controller="Home" asp-fragment="services">Courses</a>
                        <ul>
                            <li class="drop-down">
                                <a asp-action="MicorSoft" asp-controller="Home" asp-fragment="">MicorSoft</a>
                                <ul>
                                    <li><a asp-action="MicorSoft" asp-controller="Home" asp-fragment="">MOS 2016/2019</a></li>
                                    <li><a asp-action="MicorSoft" asp-controller="Home" asp-fragment="">Office 365 2016/2019</a></li>
                                    <li><a asp-action="MicorSoft" asp-controller="Home" asp-fragment="">MCSA Windows Sever 2016</a></li>
                                    <li><a asp-action="MicorSoft" asp-controller="Home" asp-fragment="">MCSA SQL Sever 2014/2016</a></li>
                                    <li><a asp-action="MicorSoft" asp-controller="Home" asp-fragment="">MCSA Web Application</a></li>
                                    <li><a asp-action="MicorSoft" asp-controller="Home" asp-fragment="">Azure Data Scientist Associate</a></li>
                                </ul>
                            </li>
                            <li class="drop-down">
                                <a asp-action="Oracle" asp-controller="Home" asp-fragment="">Oracle</a>
                                <ul>
                                    <li><a asp-action="Oracle" asp-controller="Home" asp-fragment="">Oracle Cloud Infrastructure</a></li>
                                    <li><a asp-action="Oracle" asp-controller="Home" asp-fragment="">Oracle Database</a></li>
                                    <li><a asp-action="Oracle" asp-controller="Home" asp-fragment="">Oracle Enterprise Management</a></li>

                                </ul>
                            </li>
                            <li class="drop-down">
                                <a asp-action="Cicso" asp-controller="Home" asp-fragment="">Cicso</a>
                                <ul>
                                    <li><a asp-action="Cicso" asp-controller="Home" asp-fragment="">CCNA</a></li>
                                    <li><a asp-action="Cicso" asp-controller="Home" asp-fragment="">CCNA Cyber Ops</a></li>
                                    <li><a asp-action="Cicso" asp-controller="Home" asp-fragment="">CCNP Enterprise</a></li>

                                </ul>
                            </li>
                            <li class="drop-down">
                                <a asp-action="Tableau" asp-controller="Home" asp-fragment="">Tableau</a>
                                <ul>
                                    <li><a asp-action="Tableau" asp-controller="Home" asp-fragment="">Tableau Desktop Training</a></li>
                                    <li><a asp-action="Tableau" asp-controller="Home" asp-fragment="">Tableau Data Visualization & Machine Learning</a></li>
                                </ul>
                            </li>
                            <li><a asp-action="Python" asp-controller="Home" asp-fragment="">Python</a></li>
                            <li><a asp-action="RPrograming" asp-controller="Home" asp-fragment="">R-Programing</a></li>

                        </ul>
                    </li>
                    <li><a asp-action="Index" asp-controller="Home" asp-fragment="why-us">Why Us</a></li>
                    <li class="drop-down">
                        <a asp-action="Index" asp-controller="Home" asp-fragment="softwareServices">Services</a>
                        <ul>
                            <li><a asp-action="SoftwareServices" asp-controller="Home" asp-fragment="#">Application Development</a></li>
                            <li><a asp-action="SoftwareServices" asp-controller="Home" asp-fragment="#">Web Design & Development</a></li>
                            <li><a asp-action="SoftwareServices" asp-controller="Home" asp-fragment="#">Mobile Application Development</a></li>
                        </ul>
                    </li>
                    <li><a asp-action="Index" asp-controller="Home" asp-fragment="businesspartner">Business Partners</a></li>
                    <li><a asp-action="ContactUs" asp-controller="Home">Contact Us</a></li>
                    <li>
                        <a asp-action="Index" asp-controller="ContactUs">
                            <i class="fa fa-user-circle-o" style="color: #ff689b;" data-toggle="tooltip" data-placement="bottom" title="Login/Register"></i>
                        </a>
                    </li>

                </ul>
            </nav><!-- .main-nav -->

        </div>
    </header><!-- #header -->
    <!--==========================
      Intro Section
    ============================-->
    <section id="Contanctintro" class="clearfix">
        @*<div class="container">

                <div class="intro-img">
                    <img src="/img/intro-img.svg" alt="" class="img-fluid">
                </div>

                <div class="intro-info">
                    <h2>We provide<br><span>Courses</span><br>that fit for your career growth!</h2>
                    <div>
                        <a href="#about" class="btn-get-started scrollto">Enquiry</a>
                        <a href="#services" class="btn-services scrollto">Our Courses</a>
                    </div>
                </div>

            </div>*@
